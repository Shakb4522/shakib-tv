{
  "appId": "com.shakibtv.app",
  "appName": "ShakibTV",
  "webDir": "public",
  "server": {
    "androidScheme": "https",
    "iosScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#141414",
      "showSpinner": false
    }
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true
  },
  "ios": {
    "contentInset": "automatic"
  }
}
